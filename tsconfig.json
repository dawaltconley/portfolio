{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "~/*": ["*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@data/*": ["data/*"],
      "@layouts/*": ["layouts/*"],
      "@browser/*": ["scripts/browser/*"],
      "@build/*": ["scripts/node/*"],
      "@hooks/*": ["scripts/hooks/*"],
      "@styles/*": ["styles/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
